<div class="relative overflow-hidden w-screen lg:w-full <%= wrapperClassName %>">
	<img
			class="w-full fill-current text-br-3-10"
			alt="info-card"
			src="<%= pictureUrl %>"
			width="<%= width %>"
			height="<%= height %>"
			placeholder="blur"
			/>
			<div class="absolute bottom-2 flex flex-col w-full h-full items-center justify-center">
				<% if (cta) { %>
				<div>
					<%- include('CTAButton', {
						isHighLighted: false,
						label: cta
					}) %>
				</div>
				<% } %>
			</div>
</div>
